{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.personalphotomap.co.uk/api/$1"
    },
    {
      "source": "/((?!api).)*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}